view{
  
  box-sizing: border-box;
}
.line{
  
  width: 0.5vw;
  height: 1.1em;
  background-color: #ffffff;
}
.bottom_line{
  
  width: 0.3vw;
  height: 0.8em;
  background-color: #eeeeee;
}
.table_title{
 display: flex;
}
.top{
   display: flex;
  flex-direction: column;
  color: #ffffff;
  padding: 8vw;
  background-color: rgb(106,192,227);
}
.top_one{
   display: flex;
  align-items: center;
  justify-content: space-between;
}
.top_two{
   display: flex;
  margin-top: 7vw;
  justify-content: flex-start;
}
.text{
   display: flex;
  justify-content: center;
  font-size: 0.8em;
  color: #999999;
  padding: 2vw 0;
}

.table{
   display: flex;
  flex-direction: column;
  background-color: #f6f6f6;
  padding-bottom: 0.2vw;
}

.table_one,.table_two,.table_three,.table_four{
  
  color: #aaaaaa;
  font-size: 0.9em;
  padding: 2vw 0;
  display: flex;
  justify-content: center;
}
.table_one{
   display: flex;
  flex: 2;
}
.table_two,.table_three,.table_four{
   display: flex;
  flex: 1;
}
.table_title_content{
   display: flex;
  margin-bottom: 0.2vw;
}
.j{
   display: flex;
  background-color: #ffffff;
  height: 25px;
}
.jr{
   display: flex;
   height: 25px;
  margin-right: 0.3vw;
}

.bottom{
   display: flex;
  border-top: 0.5vw solid rgb(117,196,214);
  box-sizing: border-box;
  width:100%;
  size:100%;
  position: fixed;
  bottom: 0;
  left: 0;
  justify-content: space-between;
  align-items: center;
  font-size: 0.8em;
  color: #aaaaaa;

}
